import numpy
from numpy import array
import numpy
#creating a 2d numpy array of ones
a = numpy.ones((2,4))
print("This is the shape of the array",a.shape)
#looking at the structure of the array
print(a)

#convert the whole array into a list
b = a.tolist()

#this is how the list of lists looks like
print(b)

#taking the index 0 to confirm
print(type(b)) 	
print(type(b[0]))